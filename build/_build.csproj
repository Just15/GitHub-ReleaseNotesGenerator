<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace></RootNamespace>
    <NoWarn>CS0649;CS0169</NoWarn>
    <NukeRootDirectory>..</NukeRootDirectory>
    <NukeScriptDirectory>..\src</NukeScriptDirectory>
    <NukeTelemetryVersion>1</NukeTelemetryVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
    <PackageReference Include="Nuke.Common" Version="6.1.1" />
    <PackageDownload Include="GitVersion.Tool" Version="[5.6.7]" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\src\GitHubReleaseNotesGenerator\GitHubReleaseNotesGenerator.csproj" />
  </ItemGroup>

</Project>
